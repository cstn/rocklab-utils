{
  "name": "@rocklab/utils",
  "version": "2.0.0",
  "description": "JavaScript utils",
  "keywords": [
    "rocklab",
    "javascript",
    "utils",
    "react",
    "redux"
  ],
  "author": "Carsten Stein <carsten.stein@cast-it.de>",
  "license": "MIT",
  "homepage": "https://github.com/cstn/rocklab-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cstn/rocklab-utils.git"
  },
  "bugs": {
    "url": "https://github.com/cstn/rocklab-utils/issues"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 6"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "postinstall": "npm run bootstrap",
    "compile": "lerna run compile",
    "build": "lerna run build",
    "lerna": "lerna",
    "test": "lerna run test",
    "test:coverage": "lerna run test:coverage",
    "bootstrap": "lerna bootstrap",
    "version": "lerna version --conventional-commits",
    "publish": "lerna publish",
    "release": "lerna version --conventional-commits --create-release github",
    "update": "lerna exec ncu",
    "lint:js": "eslint packages/**/src/",
    "lint": "npm run lint:js",
    "lint:fix": "npm run lint:js -- --fix",
    "prettier": "prettier --write .",
    "commit": "commit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write .",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.16.7",
    "@babel/core": "7.16.7",
    "@babel/eslint-parser": "7.16.5",
    "@babel/preset-env": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-lerna-scopes": "16.0.0",
    "@commitlint/prompt-cli": "16.1.0",
    "@rocklab/commitlint-config": "3.2.1",
    "@rocklab/eslint-config-typescript": "3.2.4",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-jest": "^27.0.2",
    "eslint": "8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.4.7",
    "jest-junit": "^12.2.0",
    "lerna": "^4.0.0",
    "lint-staged": "12.1.7",
    "prettier": "2.5.1",
    "react": "^17.0.2",
    "reselect": "^4.0.0",
    "ts-jest": "27.1.2",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.16.7",
    "@babel/polyfill": "^7.8.7"
  }
}
