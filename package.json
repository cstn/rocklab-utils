{
  "name": "@rocklab/utils",
  "version": "0.2.1",
  "description": "JavaScript utils",
  "engines": {
    "node": ">= 12",
    "npm": ">= 6"
  },
  "main": "./es/index.js",
  "browser": "./dist/index.js",
  "scripts": {
    "test": "jest --config config/jest.config.js",
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist --ignore \"**/*.test.js\"",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore \"**/*.test.js\"",
    "prepublish": "npm run build",
    "lint:js": "eslint src/",
    "lint:styles": "stylelint packages/**/*.css",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:styles -- --fix",
    "prettier": "prettier-eslint --write \"src/**/*.{js,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cstn/rocklab-utils.git"
  },
  "keywords": [
    "rocklab",
    "javascript",
    "utils"
  ],
  "author": "Carsten Stein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cstn/rocklab-utils/issues"
  },
  "homepage": "https://github.com/cstn/rocklab-utils#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@rocklab/eslint-config-rocklab": "1.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^5.16.0",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.13.1",
    "reselect": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "reselect": "^4.0.0"
  }
}
