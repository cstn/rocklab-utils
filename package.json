{
  "name": "@rocklab/utils",
  "version": "0.3.1",
  "description": "JavaScript utils",
  "keywords": [
    "rocklab",
    "javascript",
    "utils",
    "react",
    "redux"
  ],
  "author": "Carsten Stein <carsten.stein@cast-it.de>",
  "license": "MIT",
  "homepage": "https://github.com/cstn/rocklab-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cstn/rocklab-utils.git"
  },
  "bugs": {
    "url": "https://github.com/cstn/rocklab-utils/issues"
  },
  "engines": {
    "node": ">= 12",
    "npm": ">= 6"
  },
  "scripts": {
    "test": "lerna run test",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:publish": "lerna publish",
    "lint:js": "eslint packages/**/src/",
    "lint": "npm run lint:js",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:styles -- --fix",
    "prettier": "prettier-eslint --write \"$PWD/packages/**/src/**/*.{js,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@rocklab/eslint-config-rocklab": "1.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "jest-junit": "^10.0.0",
    "lerna": "^3.21.0",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "prettier-eslint-cli": "^5.0.0"
  }
}
