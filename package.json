{
  "name": "@rocklab/utils",
  "version": "1.1.31",
  "description": "JavaScript utils",
  "keywords": [
    "rocklab",
    "javascript",
    "utils",
    "react",
    "redux"
  ],
  "author": "Carsten Stein <carsten.stein@cast-it.de>",
  "license": "MIT",
  "homepage": "https://github.com/cstn/rocklab-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cstn/rocklab-utils.git"
  },
  "bugs": {
    "url": "https://github.com/cstn/rocklab-utils/issues"
  },
  "engines": {
    "node": ">= 18",
    "npm": ">= 9"
  },
  "workspaces": [
    "packages/common.utils",
    "packages/react-utils",
    "packages/redux-utils"
  ],
  "scripts": {
    "prepare": "is-ci || husky install",
    "compile": "lerna run compile",
    "build": "lerna run build",
    "lerna": "lerna",
    "test": "lerna run test",
    "test:coverage": "lerna run test:coverage",
    "version": "lerna version --conventional-commits --message \"chore: bump version\"",
    "publish": "lerna publish --message \"chore: publish %s\"",
    "release": "lerna version --conventional-commits --create-release github --message \"chore: release %s\"",
    "update": "lerna exec ncu",
    "lint:js": "eslint packages/**/src/",
    "lint": "npm run lint:js",
    "lint:fix": "npm run lint:js -- --fix",
    "prettier": "prettier --write .",
    "commit": "commit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write .",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.22.9",
    "@babel/core": "7.22.9",
    "@babel/eslint-parser": "7.22.9",
    "@babel/preset-env": "7.22.9",
    "@babel/preset-typescript": "7.22.5",
    "@commitlint/cli": "17.6.7",
    "@commitlint/prompt-cli": "17.6.7",
    "@rocklab/commitlint-config": "3.2.73",
    "@rocklab/eslint-config-typescript": "3.2.74",
    "@types/jest": "28.1.8",
    "@types/node": "18.17.0",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "babel-jest": "28.1.3",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "28.1.3",
    "jest-junit": "16.0.0",
    "lerna": "7.1.4",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "react": "17.0.2",
    "reselect": "4.1.8",
    "ts-jest": "28.0.8",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.22.9",
    "@babel/polyfill": "7.12.1"
  }
}
