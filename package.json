{
  "name": "@rocklab/utils",
  "version": "1.1.12",
  "description": "JavaScript utils",
  "keywords": [
    "rocklab",
    "javascript",
    "utils",
    "react",
    "redux"
  ],
  "author": "Carsten Stein <carsten.stein@cast-it.de>",
  "license": "MIT",
  "homepage": "https://github.com/cstn/rocklab-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cstn/rocklab-utils.git"
  },
  "bugs": {
    "url": "https://github.com/cstn/rocklab-utils/issues"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 8"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "postinstall": "npm run bootstrap",
    "compile": "lerna run compile",
    "build": "lerna run build",
    "lerna": "lerna",
    "test": "lerna run test",
    "test:coverage": "lerna run test:coverage",
    "bootstrap": "lerna bootstrap",
    "version": "lerna version --conventional-commits --message \"chore: bump version\"",
    "publish": "lerna publish --message \"chore: publish %s\"",
    "release": "lerna version --conventional-commits --create-release github --message \"chore: release %s\"",
    "update": "lerna exec ncu",
    "lint:js": "eslint packages/**/src/",
    "lint": "npm run lint:js",
    "lint:fix": "npm run lint:js -- --fix",
    "prettier": "prettier --write .",
    "commit": "commit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write .",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.2",
    "@babel/eslint-parser": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "7.17.12",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-lerna-scopes": "17.0.2",
    "@commitlint/prompt-cli": "17.0.0",
    "@rocklab/commitlint-config": "3.2.15",
    "@rocklab/eslint-config-typescript": "3.2.18",
    "@types/jest": "27.5.2",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "babel-jest": "27.5.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "jest": "27.5.1",
    "jest-junit": "13.2.0",
    "lerna": "4.0.0",
    "lint-staged": "13.0.1",
    "prettier": "2.6.2",
    "react": "17.0.2",
    "reselect": "4.1.6",
    "ts-jest": "27.1.5",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.18.2",
    "@babel/polyfill": "7.12.1"
  }
}
