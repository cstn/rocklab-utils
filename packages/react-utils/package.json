{
  "name": "@rocklab/react-utils",
  "version": "0.5.1",
  "description": "React utils",
  "keywords": [
    "rocklab",
    "javascript",
    "utils",
    "react"
  ],
  "author": "Carsten Stein <carsten.stein@cast-it.de>",
  "license": "MIT",
  "homepage": "https://github.com/cstn/rocklab-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cstn/rocklab-utils.git"
  },
  "bugs": {
    "url": "https://github.com/cstn/rocklab-utils/issues"
  },
  "engines": {
    "node": ">= 12",
    "npm": ">= 6"
  },
  "main": "./dist/index.js",
  "module": "./es/index.js",
  "files": [
    "dist",
    "src",
    "es"
  ],
  "scripts": {
    "test": "jest --config config/jest.config.js",
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist --ignore \"**/*.test.js\"",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore \"**/*.test.js\"",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@rocklab/eslint-config-react": "^2.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "cross-env": "^7.0.3",
    "jest": "^27.0.4",
    "jest-junit": "^12.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1"
  },
  "peerDependencies": {
    "prop-types": ">= 15.7",
    "react": ">=17.0",
    "react-dom": ">=17.0"
  },
  "gitHead": "bdae86ff0a5383fc5b7f2b616e5894d3c5252a6f"
}
