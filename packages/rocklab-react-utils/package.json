{
  "name": "@rocklab/react-utils",
  "version": "0.1.0",
  "description": "Utils for react web apps",
  "keywords": [
    "rocklab",
    "javascript",
    "react",
    "utils"
  ],
  "engines": {
    "node": ">= 12",
    "npm": ">= 6"
  },
  "main": "index.js",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist --ignore \"**/*.test.js\"",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore \"**/*.test.js\"",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cstn/rocklab-utils",
    "directory": "packages/rocklab-react-utils"
  },
  "bugs": {
    "url": "https://github.com/cstn/rocklab-utils/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Carsten Stein <carsten.stein@cast-it.de>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "react": "^16.13.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "cross-env": "^7.0.2"
  }
}
